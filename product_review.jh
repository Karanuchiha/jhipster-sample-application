

entity Product{
	productId String
    productName String
    descroption String
    price Integer
    stock Integer
}

entity Review{
    reviewId String
    productId String
    content String
    rating Integer
}
relationship ManyToOne{
   Product{review(productId)} to Review{product(productId)}
   
}
relationship OneToMany{
   Review{product(productId)} to  Product{review(productId)} 
   
}